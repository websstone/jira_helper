{
	"manifest_version": 2,

    "name" : "JIRA Helper", 
    "version" : "1.0", 
    "description" : "This is a simple chrome extention for help you working with JIRA", 

    "permissions": [
    	"https://jira.epam.com/"
    ],

    "browser_action" : {
    	"default_title":"JIRA Helper",
    	"default_icon":"images/logo.png",
    	"default_popup":"popup.html"
    },

    "options_page":"options.html"
}